{
    "name": "carlalexander/cli",
    "description": "Placeholder command-line tool",
    "type" : "project",
    "authors" : [
        {
            "name": "Carl Alexander",
            "email": "contact@carlalexander.ca",
            "homepage": "https://carlalexander.ca"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-pcntl": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.4",
        "ramsey/uuid": "^3.9",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/process": "^4.4",
        "symfony/yaml": "^4.4",
        "tightenco/collect": "^6.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpmd/phpmd": "^2.7",
        "phpro/grumphp": "^0.16.2",
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^8.4",
        "sebastian/phpcpd": "^4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Placeholder\\Cli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Placeholder\\Cli\\Tests\\": "tests"
        }
    }
}
